            <string>/// Draw text

if (text != "") {
    words = string_copy(text, 0, i);
    if (i &lt; string_length(text)) {
        i++;
    }
    
    if (i == string_length(text)) {
    
        // flicker = words + "|";
        flicker = words;
        
        // Allow player to exit
        endl += 1;
        
        if (endl mod 10 == 0) {
            draw_text(100, 100, flicker);
            // text = string_replace(text, '|', 'World');
        } else {
            draw_set_color(c_lime);   
            draw_text(100, 100, words);
            draw_set_color(c_lime);
        }
        
        if (endl &gt; 100) endl = 0;
        
        if (keyboard_check_released(vk_enter) || keyboard_check_released(vk_space)) {
            if (!instance_exists(obj_input)) {
                obj_console.slide += 1;
                obj_console.write = true;
                obj_console.i += 1;
                if (obj_console.i mod 14 == 0) {
                    text = "";
                    i = 0;
                }
            }
        }
    } else {
        draw_set_color(c_lime);   
        draw_text(100, 100, words);
        draw_set_color(c_lime);
        last = words;
        if (keyboard_check_released(vk_enter) || keyboard_check_released(vk_space)) {
            i = string_length(text);
        }
    }
}


</string>

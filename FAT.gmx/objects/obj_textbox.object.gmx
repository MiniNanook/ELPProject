            <string>/// Draw text

if (text != "") {
    words = string_copy(text, 0, i);
    if (i &lt; string_length(text)) {
        i++;
        if (i mod 3 == 0) {
            audio_sound_gain(snd_text, 0.1, 0);
            audio_play_sound(snd_text, 1, 0);
        }
    }
    draw_set_color(c_white);
    
    draw_set_halign(fa_center);
    
    draw_text_ext(912, 580, words, 20, 300);
    
    draw_set_halign(fa_left);
    
    draw_set_color(c_black);
    
    if (i == string_length(text)) {
        // Allow player to exit
        
        if (keyboard_check_released(vk_space) || keyboard_check_released(vk_enter)) {
            obj_player.freeze = false;
            if (item != "") {
                item.active = false;
                obj_controller.item = item;
                if (global.clicks == 7) {
                    global.clicks+= 1;
                    obj_controller.last = true;
                    obj_controller.message = true;
                }
            }
            i = 0;
            instance_destroy();
        }
    }
}


</string>

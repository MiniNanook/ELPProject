            <string>/// Draw

x = room_width / 2;
y = room_height / 2;

draw_set_font(fnt_game_large);
draw_set_color(c_white);
draw_set_halign(fa_center);

if (slide &gt;= array_length_1d(text)) {
    switch(global.story) {
        case 1:
            global.story = 2;
            global.bw = false;
            room_goto(MAIN);
        break;
        case 2:
            global.story = 3;
            global.bw = false;
            room_goto(MAIN);
        break;
        case 3:
            room_goto(CREDITS);
        break;
    }
} else {
    if (i &lt; string_length(text[slide])) {
        if (i mod 3 == 0) audio_play_sound(snd_text, 1, 0);
        i++;
    }
    words = string_copy(text[slide], 0, i);
    draw_text(x, y, words);
    
    if (i &gt;= string_length(text[slide])) {
        if (keyboard_check_released(vk_enter)) {
            if (slide &lt; array_length_1d(text)) {
                slide += 1;
                i = 0;
            }
        }
    }

}

if (global.debug) {
    if (keyboard_check_released(ord("N"))) {
        global.bw = false;
        global.story += 1;
        room_goto(MAIN);
    }
}


